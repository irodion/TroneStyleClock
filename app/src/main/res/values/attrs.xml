<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="ClockView">
        <attr name="drawGrid" format="boolean" />
        <attr name="hoursColor" format="reference|color" />
        <attr name="minutesColor" format="reference|color" />
        <attr name="secondsColor" format="reference|color" />
        <attr name="arcWidth" format="dimension" />
        <attr name="arcPadding" format="dimension" />
        <attr name="hourPointerDegrees" format="integer" />
        <attr name="minutesPointerDegrees" format="integer" />
        <attr name="secondsPointerDegrees" format="integer" />
    </declare-styleable>
</resources>